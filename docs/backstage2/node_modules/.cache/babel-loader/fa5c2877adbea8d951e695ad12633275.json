MAFA;AAGA,cAAA,OAAA,CAAA,GAAA,CAAA,GAAA,EAAA,MAAA;;AACA,kBAAA,MAAA,KAAA,GAAA,EAAA;AACA;AACA,oBAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,gBAAA,OAAA,CAAA,GAAA,CAAA,KAAA,EAHA,CAKA;;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,kBAAA,IAAA,EAAA;AAAA,iBAAA,EANA,CAQA;;;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA;AACA,kBAAA,OAAA,EAAA,MADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA;AAIA,eAbA,MAaA;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA;AACA,kBAAA,OAAA,EAAA,GADA;AAEA,kBAAA,IAAA,EAAA;AAFA,iBAAA;;AAIA,gBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA;AACA;AACA,aAxBA,EAyBA,KAzBA,CAyBA,UAAA,KAAA,EAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,aA3BA;AA4BA;AACA,SAjCA,MAiCA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,MAAA;AACA,iBAAA,KAAA;AACA;AACA,OAtCA;AAuCA,KAzCA;AA0CA,IAAA,SA1CA,qBA0CA,QA1CA,EA0CA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,WAAA;AACA;AA5CA;AAnBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"login\">\r\n        <div class=\"login_box\">\r\n            <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"80px\" class=\"demo-ruleForm\">\r\n                <el-form-item label=\"账号\" prop=\"username\">\r\n                    <el-input v-model=\"ruleForm.username\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"密码\" prop=\"password\">\r\n                    <el-input type=\"password\" v-model=\"ruleForm.password\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item class=\"el-form-item_btn\">\r\n                    <el-button type=\"primary\" @click=\"submitForm('ruleForm')\">登录</el-button>\r\n                    <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { goLogin } from '../../http/api'\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      ruleForm: {\r\n        username: '',\r\n        password: ''\r\n      },\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入账号', trigger: 'blur' },\r\n          { min: 5, max: 25, message: '长度在 5 到 25 个字符', trigger: 'blur' }\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n          { min: 5, max: 25, message: '长度在 5 到 25 个字符', trigger: 'blur' }\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    submitForm (formName) {\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          console.log('登录成功', this.ruleForm)\r\n          if (valid) {\r\n            console.log(this.ruleForm)\r\n            goLogin(this.ruleForm).then(res => {\r\n              console.log(res)\r\n              var { msg, status } = res.data.meta\r\n              console.log(msg, status)\r\n              if (status === 200) {\r\n                // 存token\r\n                var token = res.data.data.token\r\n                console.log(token)\r\n\r\n                // 跳转页面\r\n                this.$router.push({ name: 'Home' })\r\n\r\n                // 登录成功提示\r\n                this.$message({\r\n                  message: '登录成功',\r\n                  type: 'success'\r\n                })\r\n              } else {\r\n                this.$message({\r\n                  message: msg,\r\n                  type: 'success'\r\n                })\r\n                this.$message.error(msg)\r\n              }\r\n            })\r\n              .catch(error => {\r\n                console.log(error)\r\n              })\r\n          }\r\n        } else {\r\n          console.log('登录失败')\r\n          return false\r\n        }\r\n      })\r\n    },\r\n    resetForm (formName) {\r\n      this.$refs[formName].resetFields()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.login{\r\n    width: 100vw;height: 100vh;\r\n    background-color: #2D3A4B;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n.login_box{\r\n    width: 450px;height: 335px;\r\n    background-color: pink;\r\n}\r\n.demo-ruleForm{\r\n    margin: 120px 0px auto;\r\n}\r\n.el-form-item_btn{\r\n    text-align: center;\r\n    margin-left: -80px;\r\n}\r\n.el-input{\r\n    width: 90%;\r\n}\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/login"}]}